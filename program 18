#include <stdio.h>

int main() {
    int day;

    printf("Enter day number (1â€“7): ");
    scanf("%d", &day);

    if (day >= 1 && day <= 5) {
        printf("It is a Working Day.\n");
    } 
    else if (day == 6 || day == 7) {
        printf("It is a Weekend.\n");
    } 
    else {
        printf("Invalid day number.\n");
    }

    return 0;
}