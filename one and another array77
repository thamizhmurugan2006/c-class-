#include <stdio.h>

int main() {
    int n, i;
    int arr1[100], arr2[100];

    printf("Enter number of elements: ");
    scanf("%d", &n);

    // Input elements for first array
    printf("Enter %d elements for first array:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &arr1[i]);
    }

    // Copy elements from arr1 to arr2
    for(i = 0; i < n; i++) {
        arr2[i] = arr1[i];
    }

    // Display first array
    printf("\nFirst Array Elements:\n");
    for(i = 0; i < n; i++) {
        printf("%d ", arr1[i]);
    }

    // Display second array
    printf("\nSecond Array Elements (Copied):\n");
    for(i = 0; i < n; i++) {
        printf("%d ", arr2[i]);
    }

    return 0;
}