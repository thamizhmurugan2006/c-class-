#include <stdio.h>

// Function to calculate factorial
int factorial(int n) {
    int i, fact = 1;

    for (i = 1; i <= n; i++) {
        fact = fact * i;
    }

    return fact;
}

int main() {
    int number, result;

    printf("Enter a number: ");
    scanf("%d", &number);

    // Function call
    result = factorial(number);

    printf("\nFactorial of %d = %d\n", number, result);

    return 0;
}