#include <stdio.h>

int main() {
    char str[100];
    int i = 0, vowelCount = 0;

    printf("Enter a string: ");
    fgets(str, sizeof(str), stdin);

    // Count vowels
    while (str[i] != '\0') {
        char ch = str[i];

        if (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u' ||
            ch == 'A' || ch == 'E' || ch == 'I' || ch == 'O' || ch == 'U') {
            vowelCount++;
        }
        i++;
    }

    printf("Number of vowels in the string = %d\n", vowelCount);

    return 0;
}