#include <stdio.h>

int main() {
    int marks;

    printf("Enter your marks (0â€“100): ");
    scanf("%d", &marks);

    if (marks < 0 || marks > 100) {
        printf("Invalid marks entered.\n");
    } 
    else if (marks >= 90) {
        printf("Grade: A\n");
    } 
    else if (marks >= 80) {
        printf("Grade: B\n");
    } 
    else if (marks >= 70) {
        printf("Grade: C\n");
    } 
    else if (marks >= 60) {
        printf("Grade: D\n");
    } 
    else if (marks >= 40) {
        printf("Grade: E\n");
    } 
    else {
        printf("Grade: F (Fail)\n");
    }

    return 0;
}